import java.util.Scanner;

public class exercise4 {

    public static void main(String[] args) {
        System.out.println("Swap the integer and fractional part of a number");
        int n, d;
        Scanner in = new Scanner(System.in);
        System.out.print("Enter n= " );
        n = in.nextInt();
        System.out.print("Enter d= " );
        d = in.nextInt();
        System.out.println ("The number you entered is " + n + n + n + "." + d + d +d );
        System.out.println ("The  number you got is " + d +d+d +"."+n+n+n);
        in.close();
    }
}
